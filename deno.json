  "shadcn-ui": {
    "moduleResolution": "NodeNext",
    "compilerOptions": {
      "paths": {
        "@/*": [
          "./*"
        ]
      }
    },
    "components": {
      "$schema": "https://ui.shadcn.com/schema.json",
      "style": "new-york",
      "rsc": true,
      "tsx": true,
      "tailwind": {
        "config": "tailwind.config.ts",
        "css": "static/assets/globals.css",
        "baseColor": "gray",
        "cssVariables": true,
        "prefix": ""
      },
      "aliases": {
        "components": "@/src/components",
        "utils": "@/src/lib/utils.ts"
      }
    }
  }
